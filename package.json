{
    "name": "RatHigh",
    "displayName": "RAT Highlights",
    "description": "Syntax highlight for the RAT language",
    "version": "1.0.0",
    "publisher": "DinoGurnari",
    "icon": "assets/logo.jpg",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "rat",
            "aliases": ["Rat", "rat", "RAT"],
            "extensions": [".rat"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rat",
            "scopeName": "source.rat",
            "path": "./syntaxes/rat.tmLanguage.json"
        }]
    }
}